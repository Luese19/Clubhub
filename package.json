{
  "name": "clubhub:-the-all-in-one-school-club-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "cd server && npm install && npm run build",
    "preview": "vite preview",
    "start": "cd server && npm start",
    "dev:server": "cd server && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "install:all": "npm install && cd server && npm install",
    "migrate": "cd server && npm run migrate",
    "heroku-postbuild": "npm run build:client && cd server && npm install && npm run build && cd .. && ls -la dist"
  },
  "dependencies": {
    "@google/genai": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
